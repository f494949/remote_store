#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/GitStore/project/vue_php/interact/node_modules/nanoid/bin/node_modules:/mnt/d/GitStore/project/vue_php/interact/node_modules/nanoid/node_modules:/mnt/d/GitStore/project/vue_php/interact/node_modules:/mnt/d/GitStore/project/vue_php/node_modules:/mnt/d/GitStore/project/node_modules:/mnt/d/GitStore/node_modules:/mnt/d/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/GitStore/project/vue_php/interact/node_modules/nanoid/bin/node_modules:/mnt/d/GitStore/project/vue_php/interact/node_modules/nanoid/node_modules:/mnt/d/GitStore/project/vue_php/interact/node_modules:/mnt/d/GitStore/project/vue_php/node_modules:/mnt/d/GitStore/project/node_modules:/mnt/d/GitStore/node_modules:/mnt/d/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
